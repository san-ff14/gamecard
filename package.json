{
  "name": "MYNUXT",
  "version": "1.0.0",
  "description": "MYNUXT",
  "author": "san",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "wc": "VUE_CLI_CSS_SHADOW_MODE=true vue-cli-service build --target wc-async --name my-components ./src/plugins/*.vue --dest ./web-components",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "cross-env": "^5.2.0",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/auth": "^4.8.1",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@vue/cli": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel": "^6.23.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "basic-auth": "^2.0.1",
    "consola": "^2.9.0",
    "element-ui": "^2.11.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "form-data": "^2.5.0",
    "graphql": "^14.4.2",
    "jquery": "^3.4.1",
    "multer": "^1.4.1",
    "nodemon": "^1.19.1",
    "nuxt-stylus-resources-loader": "^2.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sequelize": "^5.10.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "unzip-stream": "^0.3.0"
  }
}
